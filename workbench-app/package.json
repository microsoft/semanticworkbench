{
    "name": "semantic-workbench-app",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "packageManager": "pnpm@9.10.0",
    "scripts": {
        "build": "tsc && vite build",
        "depcheck": "depcheck --ignores=\"@types/*,ts-prune,typescript,vite\" --ignore-dirs=\".vscode,.vs,.git,node_modules\" --skip-missing",
        "dev": "vite",
        "find-deadcode": "node ./tools/filtered-ts-prune.cjs",
        "format": "prettier --write src",
        "lint": "eslint src --fix",
        "prettify": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,scss,css,html,svg}\"",
        "preview": "vite preview",
        "serve": "http-server dist -p 4000 -S -C certs/cert.pem -K certs/key.pem",
        "start": "vite",
        "type-check": "tsc"
    },
    "dependencies": {
        "@azure/ms-rest-js": "^2.7.0",
        "@azure/msal-browser": "^3.28.1",
        "@azure/msal-react": "^2.2.0",
        "@fluentui-copilot/react-attachments": "^0.10.5",
        "@fluentui-copilot/react-copilot": "^0.19.4",
        "@fluentui/keyboard-keys": "^9.0.8",
        "@fluentui/react-components": "^9.59.0",
        "@fluentui/react-context-selector": "^9.1.72",
        "@fluentui/react-file-type-icons": "^8.12.7",
        "@fluentui/react-icons": "^2.0.274",
        "@fluentui/react-jsx-runtime": "^9.0.50",
        "@fluentui/react-motions-preview": "^0.2.8",
        "@fluentui/react-shared-contexts": "^9.21.2",
        "@fluentui/react-tabster": "^9.23.3",
        "@fluentui/react-utilities": "^9.18.20",
        "@git-diff-view/file": "^0.0.16",
        "@git-diff-view/react": "^0.0.16",
        "@lexical/utils": "^0.17.1",
        "@microsoft/fetch-event-source": "^2.0.1",
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "@reduxjs/toolkit": "^1.9.7",
        "@rjsf/core": "^5.24.3",
        "@rjsf/fluentui-rc": "^5.24.3",
        "@rjsf/utils": "^5.24.3",
        "@rjsf/validator-ajv8": "^5.24.3",
        "abcjs": "^6.4.4",
        "dayjs": "^1.11.13",
        "debug": "^4.4.0",
        "deep-object-diff": "^1.1.9",
        "deepmerge": "^4.3.1",
        "diff": "^7.0.0",
        "js-tiktoken": "^1.0.19",
        "js-yaml": "^4.1.0",
        "lexical": "^0.17.1",
        "mermaid": "^10.9.3",
        "microsoft-cognitiveservices-speech-sdk": "^1.42.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-json-tree": "^0.19.0",
        "react-markdown": "^9.0.3",
        "react-redux": "^8.1.3",
        "react-router-dom": "^6.29.0",
        "react-syntax-highlighter": "^15.6.1",
        "react-virtualized-auto-sizer": "^1.0.25",
        "react-virtuoso": "^4.12.5",
        "rehype-raw": "^7.0.0",
        "rehype-sanitize": "^6.0.0",
        "remark-gfm": "^4.0.1",
        "streamsaver": "^2.0.6"
    },
    "devDependencies": {
        "@rollup/plugin-replace": "^5.0.7",
        "@types/debug": "^4.1.12",
        "@types/diff": "^5.2.3",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^20.17.19",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@types/streamsaver": "^2.0.5",
        "@typescript-eslint/parser": "^5.62.0",
        "@vitejs/plugin-react": "^4.3.4",
        "depcheck": "^1.4.7",
        "eslint": "^8.57.1",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-security": "^0.1.0",
        "prettier": "^2.8.8",
        "ts-prune": "^0.10.3",
        "typescript": "^5.7.3",
        "vite": "^6.1.0",
        "vite-plugin-mkcert": "^1.17.6"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ],
        "plugins": [
            "prettier"
        ],
        "rules": {
            "prettier/prettier": "error",
            "react/jsx-no-target-blank": "off"
        }
    },
    "pnpm": {
        "overrides": {
            "nanoid@<3.3.8": ">=3.3.8",
            "vite@<=4.5.5": ">=4.5.6",
            "katex@>=0.12.0 <=0.16.20": ">=0.16.21"
        }
    }
}
