{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "app: semantic-workbench-app",
            "cwd": "${workspaceFolder}",
            "skipFiles": ["<node_internals>/**"],
            "console": "integratedTerminal",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "dev"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "app: semantic-workbench-app (no strict mode)",
            "env": {
                "VITE_DISABLE_STRICT_MODE": "true"
            },
            "cwd": "${workspaceFolder}",
            "skipFiles": ["<node_internals>/**"],
            "console": "integratedTerminal",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "dev"]
        }
    ]
}
